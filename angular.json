{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@senzing/rest-api-client-ng": {
      "root": "projects/rest-api-client-ng",
      "sourceRoot": "projects/rest-api-client-ng/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/rest-api-client-ng/tsconfig.lib.json",
            "project": "projects/rest-api-client-ng/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/rest-api-client-ng/tsconfig.lib.prod.json"
            },
              "development": {}
          },
            "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/rest-api-client-ng/src/test.ts",
            "tsConfig": "projects/rest-api-client-ng/tsconfig.spec.json",
            "karmaConfig": "projects/rest-api-client-ng/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/rest-api-client-ng/tsconfig.lib.json",
              "projects/rest-api-client-ng/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "testlib": {
      "projectType": "library",
      "root": "projects/testlib",
      "sourceRoot": "projects/testlib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/testlib/tsconfig.lib.json",
            "project": "projects/testlib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/testlib/tsconfig.lib.prod.json"
            },
            "development": {}
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/testlib/src/test.ts",
            "tsConfig": "projects/testlib/tsconfig.spec.json",
            "karmaConfig": "projects/testlib/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/testlib/tsconfig.lib.json",
              "projects/testlib/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "@senzing/rest-api-client-ng",
  "cli": {
    "analytics": "544ec7f8-4c04-49ba-96ce-3cd59fd8763d"
  }
